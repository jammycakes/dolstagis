@model IEnumerable<Dolstagis.Accounts.User>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_2col.cshtml";
}

<h2>User list</h2>

<table>
  <tr>
    <th>
      <input type="checkbox" class="select-all" />
    </th>
    <th>User ID</th>
    <th>User name</th>
    <th>Email</th>
    <th>Name</th>
  </tr>
  @foreach (var user in Model) {
    <tr>
      <td>
        <input type="checkbox" name="select" value="@user.UserID" />
      </td>
      <td>@user.UserID</td>
      <td>@user.UserName</td>
      <td>
        <a href="mailto:@user.EmailAddress">@user.EmailAddress</a>
      </td>
      <td>@user.DisplayName</td>
    </tr>
  }
</table>